<?xml version="1.0" encoding="utf-8"?>
<set>
    <set xmlns:n1="http://schemas.android.com/apk/res/android"
        n1:ordering="sequentially">
        <objectAnimator
            n1:duration="200"
            n1:interpolator="@android:interpolator/linear"
            n1:propertyName="strokeAlpha"
            n1:valueFrom="1.0"
            n1:valueTo="1.0"
            n1:valueType="floatType" />
        <objectAnimator
            n1:duration="16"
            n1:interpolator="@android:interpolator/linear"
            n1:propertyName="strokeAlpha"
            n1:valueFrom="1.0"
            n1:valueTo="0.5"
            n1:valueType="floatType" />
        <objectAnimator
            n1:duration="633"
            n1:interpolator="@android:interpolator/linear"
            n1:propertyName="strokeAlpha"
            n1:valueFrom="0.5"
            n1:valueTo="0.5"
            n1:valueType="floatType" />
        <objectAnimator
            n1:duration="16"
            n1:interpolator="@android:interpolator/linear"
            n1:propertyName="strokeAlpha"
            n1:valueFrom="0.5"
            n1:valueTo="1.0"
            n1:valueType="floatType" />
    </set>
</set>